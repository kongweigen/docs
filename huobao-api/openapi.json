{
  "openapi": "3.0.1",
  "info": {
    "title": "Mintlify External API",
    "description": "An API for Mintlify documentation management and resource access.",
    "version": "1.0.0"
  },
  "servers": [
    {
      "url": "http://110.42.70.162:6999/v1"
    }
  ],
  "security": [
    {
      "bearerAuth": []
    }
  ],
  "x-mcp": {
    "enabled": true
  },
  "paths": {
    "/chat/completions": {
      "post": {
        "summary": "Chat Completions",
        "description": "AI问答",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "required": ["model", "messages"],
                "properties": {
                  "model": {
                    "type": "string",
                    "description": "使用的AI模型名称",
                    "example": "dall-e-3",
                    "enum": ["dall-e-3", "gpt-4", "gpt-3.5-turbo"]
                  },
                  "messages": {
                    "type": "array",
                    "description": "对话消息列表",
                    "items": {
                      "type": "object",
                      "required": ["content", "role"],
                      "properties": {
                        "content": {
                          "type": "string",
                          "description": "消息内容"
                        },
                        "role": {
                          "type": "string",
                          "description": "消息角色",
                          "enum": ["user", "assistant", "system"]
                        }
                      }
                    }
                  },
                  "group": {
                    "type": "string",
                    "description": "分组标识",
                    "default": "default",
                    "example": "default"
                  },
                  "stream": {
                    "type": "boolean",
                    "description": "是否启用流式响应",
                    "default": true
                  },
                  "temperature": {
                    "type": "number",
                    "description": "控制输出随机性的温度参数",
                    "minimum": 0,
                    "maximum": 2,
                    "default": 0.7
                  },
                  "top_p": {
                    "type": "number",
                    "description": "核采样参数",
                    "minimum": 0,
                    "maximum": 1,
                    "default": 1
                  },
                  "frequency_penalty": {
                    "type": "number",
                    "description": "频率惩罚参数",
                    "minimum": -2,
                    "maximum": 2,
                    "default": 0
                  },
                  "presence_penalty": {
                    "type": "number",
                    "description": "存在惩罚参数",
                    "minimum": -2,
                    "maximum": 2,
                    "default": 0
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Successful response",
            "content": {
              "text/event-stream": {
                "schema": {
                  "type": "string",
                  "description": "流式响应数据，每行为一个JSON对象，格式为Server-Sent Events",
                  "example": "data: {\"id\":\"chatcmpl-123\",\"object\":\"chat.completion.chunk\",\"created\":1677652288,\"model\":\"dall-e-3\",\"choices\":[{\"index\":0,\"delta\":{\"content\":\"Hello\"},\"finish_reason\":null}]}\n\ndata: [DONE]\n\n"
                }
              },
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ChatCompletionResponse"
                }
              }
            }
          },
          "400": {
            "description": "Bad Request",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ResultObject"
                }
              }
            }
          },
          "401": {
            "description": "Unauthorized",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ResultObject"
                }
              }
            }
          },
          "403": {
            "description": "Forbidden",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ResultObject"
                }
              }
            }
          },
          "404": {
            "description": "Not Found",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ResultObject"
                }
              }
            }
          },
          "405": {
            "description": "Method Not Allowed",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ResultObject"
                }
              }
            }
          },
          "500": {
            "description": "Internal Server Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ResultObject"
                }
              }
            }
          }
        }
      }
    },
    "/image/jimeng/generate": {
      "post": {
        "summary": "即梦图片生成",
        "description": "使用即梦AI生成图片",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "required": ["prompt"],
                "properties": {
                  "prompt": {
                    "type": "string",
                    "description": "图片生成提示词",
                    "example": "一只可爱的小猫在花园里玩耍"
                  },
                  "size": {
                    "type": "string",
                    "description": "图片尺寸",
                    "enum": ["1024x1024", "1024x1792", "1792x1024"],
                    "default": "1024x1024"
                  },
                  "quality": {
                    "type": "string",
                    "description": "图片质量",
                    "enum": ["standard", "hd"],
                    "default": "standard"
                  },
                  "style": {
                    "type": "string",
                    "description": "图片风格",
                    "enum": ["vivid", "natural"],
                    "default": "vivid"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Successful response",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ImageGenerateResponse"
                }
              }
            }
          },
          "400": {
            "description": "Bad Request",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ResultObject"
                }
              }
            }
          },
          "401": {
            "description": "Unauthorized",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ResultObject"
                }
              }
            }
          },
          "500": {
            "description": "Internal Server Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ResultObject"
                }
              }
            }
          }
        }
      }
    },
    "/image/jimeng/status": {
      "get": {
        "summary": "即梦任务状态查询",
        "description": "查询即梦图片生成任务状态",
        "parameters": [
          {
            "name": "task_id",
            "in": "query",
            "required": true,
            "schema": {
              "type": "string",
              "description": "任务ID"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Successful response",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ImageStatusResponse"
                }
              }
            }
          },
          "400": {
            "description": "Bad Request",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ResultObject"
                }
              }
            }
          },
          "404": {
            "description": "Task Not Found",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ResultObject"
                }
              }
            }
          }
        }
      }
    },
    "/image/keling/generate": {
      "post": {
        "summary": "可灵图片生成",
        "description": "使用可灵AI生成图片",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "required": ["prompt"],
                "properties": {
                  "prompt": {
                    "type": "string",
                    "description": "图片生成提示词",
                    "example": "一幅美丽的山水画，中国风格"
                  },
                  "aspect_ratio": {
                    "type": "string",
                    "description": "图片宽高比",
                    "enum": ["1:1", "16:9", "9:16", "4:3", "3:4"],
                    "default": "1:1"
                  },
                  "image_count": {
                    "type": "integer",
                    "description": "生成图片数量",
                    "minimum": 1,
                    "maximum": 4,
                    "default": 1
                  },
                  "mode": {
                    "type": "string",
                    "description": "生成模式",
                    "enum": ["standard", "professional"],
                    "default": "standard"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Successful response",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ImageGenerateResponse"
                }
              }
            }
          },
          "400": {
            "description": "Bad Request",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ResultObject"
                }
              }
            }
          },
          "401": {
            "description": "Unauthorized",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ResultObject"
                }
              }
            }
          },
          "500": {
            "description": "Internal Server Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ResultObject"
                }
              }
            }
          }
        }
      }
    },
    "/image/keling/status": {
      "get": {
        "summary": "可灵任务状态查询",
        "description": "查询可灵图片生成任务状态",
        "parameters": [
          {
            "name": "task_id",
            "in": "query",
            "required": true,
            "schema": {
              "type": "string",
              "description": "任务ID"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Successful response",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ImageStatusResponse"
                }
              }
            }
          },
          "400": {
            "description": "Bad Request",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ResultObject"
                }
              }
            }
          },
          "404": {
            "description": "Task Not Found",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ResultObject"
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "securitySchemes": {
      "bearerAuth": {
        "type": "http",
        "scheme": "bearer"
      }
    },
    "schemas": {
      "ResultObject": {
        "type": "object",
        "properties": {
          "code": {
            "type": "integer",
            "format": "int32",
            "description": "Response code"
          },
          "message": {
            "type": "string",
            "description": "Response message"
          },
          "data": {
            "type": "object",
            "description": "Response data"
          },
          "timestamp": {
            "type": "integer",
            "format": "int64",
            "description": "Timestamp"
          },
          "success": {
            "type": "boolean",
            "description": "Success status"
          }
        }
      },
      "ChatCompletionResponse": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "description": "聊天完成的唯一标识符"
          },
          "object": {
            "type": "string",
            "description": "对象类型",
            "enum": ["chat.completion"]
          },
          "created": {
            "type": "integer",
            "format": "int64",
            "description": "创建时间戳"
          },
          "model": {
            "type": "string",
            "description": "使用的模型名称"
          },
          "choices": {
            "type": "array",
            "description": "生成的选择列表",
            "items": {
              "type": "object",
              "properties": {
                "index": {
                  "type": "integer",
                  "description": "选择的索引"
                },
                "message": {
                  "type": "object",
                  "properties": {
                    "role": {
                      "type": "string",
                      "description": "消息角色",
                      "enum": ["assistant"]
                    },
                    "content": {
                      "type": "string",
                      "description": "消息内容"
                    }
                  }
                },
                "finish_reason": {
                  "type": "string",
                  "description": "完成原因",
                  "enum": ["stop", "length", "content_filter", "tool_calls"]
                }
              }
            }
          },
          "usage": {
            "type": "object",
            "description": "使用情况统计",
            "properties": {
              "prompt_tokens": {
                "type": "integer",
                "description": "提示词token数量"
              },
              "completion_tokens": {
                "type": "integer",
                "description": "完成内容token数量"
              },
              "total_tokens": {
                "type": "integer",
                "description": "总token数量"
              }
            }
          }
        }
      },
      "ImageGenerateResponse": {
        "type": "object",
        "properties": {
          "task_id": {
            "type": "string",
            "description": "任务唯一标识符"
          },
          "status": {
            "type": "string",
            "description": "任务状态",
            "enum": ["pending", "processing", "completed", "failed"]
          },
          "created_at": {
            "type": "integer",
            "format": "int64",
            "description": "任务创建时间戳"
          },
          "model": {
            "type": "string",
            "description": "使用的AI模型",
            "enum": ["jimeng", "keling"]
          },
          "prompt": {
            "type": "string",
            "description": "原始提示词"
          },
          "images": {
            "type": "array",
            "description": "生成的图片列表（仅在completed状态时返回）",
            "items": {
              "type": "object",
              "properties": {
                "url": {
                  "type": "string",
                  "description": "图片URL"
                },
                "width": {
                  "type": "integer",
                  "description": "图片宽度"
                },
                "height": {
                  "type": "integer",
                  "description": "图片高度"
                },
                "format": {
                  "type": "string",
                  "description": "图片格式",
                  "enum": ["png", "jpg", "webp"]
                }
              }
            }
          },
          "error": {
            "type": "object",
            "description": "错误信息（仅在failed状态时返回）",
            "properties": {
              "code": {
                "type": "string",
                "description": "错误代码"
              },
              "message": {
                "type": "string",
                "description": "错误描述"
              }
            }
          }
        }
      },
      "ImageStatusResponse": {
        "type": "object",
        "properties": {
          "task_id": {
            "type": "string",
            "description": "任务唯一标识符"
          },
          "status": {
            "type": "string",
            "description": "任务状态",
            "enum": ["pending", "processing", "completed", "failed"]
          },
          "progress": {
            "type": "integer",
            "description": "任务进度百分比 (0-100)",
            "minimum": 0,
            "maximum": 100
          },
          "created_at": {
            "type": "integer",
            "format": "int64",
            "description": "任务创建时间戳"
          },
          "updated_at": {
            "type": "integer",
            "format": "int64",
            "description": "任务最后更新时间戳"
          },
          "completed_at": {
            "type": "integer",
            "format": "int64",
            "description": "任务完成时间戳（仅在completed状态时返回）"
          },
          "model": {
            "type": "string",
            "description": "使用的AI模型",
            "enum": ["jimeng", "keling"]
          },
          "prompt": {
            "type": "string",
            "description": "原始提示词"
          },
          "images": {
            "type": "array",
            "description": "生成的图片列表（仅在completed状态时返回）",
            "items": {
              "type": "object",
              "properties": {
                "url": {
                  "type": "string",
                  "description": "图片URL"
                },
                "width": {
                  "type": "integer",
                  "description": "图片宽度"
                },
                "height": {
                  "type": "integer",
                  "description": "图片高度"
                },
                "format": {
                  "type": "string",
                  "description": "图片格式",
                  "enum": ["png", "jpg", "webp"]
                }
              }
            }
          },
          "error": {
            "type": "object",
            "description": "错误信息（仅在failed状态时返回）",
            "properties": {
              "code": {
                "type": "string",
                "description": "错误代码"
              },
              "message": {
                "type": "string",
                "description": "错误描述"
              }
            }
          }
        }
      }
    }
  }
}
